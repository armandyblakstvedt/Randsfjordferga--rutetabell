{
  "expo": {
    "name": "Fergetider",
    "slug": "Fergetider",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#99ffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fergetider",
      "buildNumber": "17"
    },
    "android": {
      "versionCode": 7,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.fergetider"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
